<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

 
 		<bean id="keyboardId" class="_08.constructor.ref.model.Keyboard">
			<constructor-arg value="K360"/>
		</bean>
 
 		<bean id="computerId" class="_08.constructor.ref.model.Computer">
 			<constructor-arg value="Acer 7741G"/>
 			<constructor-arg value="500"/>
 			<constructor-arg ref="keyboardId"/>
 		</bean>
 		
</beans>